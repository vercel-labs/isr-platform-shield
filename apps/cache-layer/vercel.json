{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "/",
			"destination": "https://core.pzvtest314.vercel.app/"
		},
		{
			"source": "/:path*",
			"destination": "https://core.pzvtest314.vercel.app/:path*"
		}
	],
	"headers": [
		{
			"source": "(.*)",
			"headers": [
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				}
			]
		}
	]
}