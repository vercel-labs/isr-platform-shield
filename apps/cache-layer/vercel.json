{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"routes": [
		{
			"src": "(.*)",
			"dest": "https://core.pzvtest314.vercel.app$1",
			"headers": {
				"CDN-Cache-Control": "s-maxage=60",
				"Vary": "Host"
			}
		},
		{
			"src": "/(.*)",
			"dest": "https://core.pzvtest314.vercel.app/s/:host/$1",
			"has": [
				{
					"type": "header",
					"key": "Sec-Fetch-Mode",
					"value": "navigate"
				},
				{
					"type": "header",
					"key": "X-Forwarded-Host",
					"value": "(?<host>.*)"
				}
			],
			"headers": {
				"CDN-Cache-Control": "s-maxage=60",
				"Vary": "X-Forwarded-Host"
			}
		}
	]
}