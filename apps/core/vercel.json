{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"headers": [
		{
			"source": "/:path*",
			"headers": [
				{
					"key": "x-vercel-enable-rewrite-caching",
					"value": "1"
				}
			]
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/$1",
			"transforms": [
				{
					"type": "response.headers",
					"op": "set",
					"target": {
						"key": "x-vercel-cache-tag"
					},
					"args": "$PRESERVE_CACHE_TAG"
				}
			]
		}
	]
}