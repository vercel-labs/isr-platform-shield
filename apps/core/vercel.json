{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"routes": [
		{
			"src": "/(.*)",
			"has": [
				{
					"type": "header",
					"key": "x-forwarded-host",
					"value": "(?<host>.*)"
				}
			],
			"transforms": [
				{
					"type": "response.headers",
					"op": "set",
					"target": {
						"key": "x-vercel-enable-rewrite-caching"
					},
					"args": "1"
				},
				{
					"type": "response.headers",
					"op": "set",
					"target": {
						"key": "vercel-cache-tag"
					},
					"args": ":host"
				}
			]
		}
	]
}